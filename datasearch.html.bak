   function m_selectAll(){
	   var i=0;
	   $$('.oDbList').each(function(obj){
									obj.set('checked',true); 
									dbItems[i].selected = true;
									++i;
									 })
	 
   }
   function m_disSelectAll(){
	   var i = 0;
	    $$('.oDbList').each(function(obj){
									obj.set('checked',false); 
									dbItems[i].selected = false;
									++i;
									 })
	  
	 
   }

